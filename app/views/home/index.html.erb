          <li><a href="/anger"><img src="./feelings/img13.png" width="30px" height="30px" class="center-block"></a></li>
        </ul>
      </div>
    </nav>
    <div id="map">
      <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBp2i1X61b72NGPlgaljJaYvP0SywGMb28&callback=initMap"></script>
    </div><!--#map-->
    <div id = "bottom_bar" class="deep-orange lighten-4">
      <div id = "icons">
        <a id="anticipacion-icon" class="btn-floating btn-small waves-effect waves-light white" onclick="flag(0)">
          <img src="./feelings/img01.png" width="40px" height="40px">
        </a>
        <a id="joy-icon" class="btn-floating btn-small waves-effect waves-light white" onclick="flag(1)">
          <img src="./feelings/img02.png" width="40px" height="40px">
