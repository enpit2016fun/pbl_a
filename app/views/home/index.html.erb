      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

      <script type="text/javascript" src="assets/home.js"></script>
      <link rel="stylesheet" href="assets/home.css" />
      <!--link rel="stylesheet" href="css/jquery-ui.css" /-->
  </head>
  <body>

    <nav>
      <div class="nav-wrapper deep-orange lighten-4">
        <a class="brand-logo left" href="/">ここlogue</a>
        <ul class="right hide-on-med-and-down">
          <li><a class="dropdown-button" href="#!" data-activates="dropdown" data-beloworigin="true">Search<i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>

        <!-- Dropdown Structure -->
        <ul id="dropdown" class="right dropdown-content collection">
          <li><a href="/anticipacion"><img src="./feelings/img01.png" width="30px" height="30px" class="center-block"></a></li>
          <li><a href="/joy"><img src="./feelings/img02.png" width="30px" height="30px" class="center-block"></a></li>
          <li><a href="/trust"><img src="./feelings/img03.png" width="30px" height="30px" class="center-block"></a></li>
          <li><a href="/fear"><img src="./feelings/img05.png" width="30px" height="30px" class="center-block"></a></li>
